{
    "name": "laragle/authentication",
    "description": "The Laragle authentication component.",
    "keywords": [
        "laragle",
        "authentication",
        "laravel"
    ],
    "homepage": "https://github.com/laragle/authentication",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Darwin Luague",
            "email": "darwinluague9001@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/support": "^7.18",
        "laravel/sanctum": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Laragle\\Auth\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laragle\\Authentication\\AuthenticationServiceProvider",
                "Laragle\\Authentication\\Passwords\\PasswordResetServiceProvider"
            ]
        }
    }
}
