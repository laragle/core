{
    "name": "laragle/core",
    "description": "Laragle core API",
    "keywords": [
        "laragle",
        "laravel"
    ],
    "homepage": "https://github.com/laragle/core",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Darwin Luague",
            "email": "darwinluague9001@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/support": "^7.18",
        "laravel/sanctum": "^2.4"
    },
    "replace": {
        "laragle/auth": "self.version"
    },
    "require-dev": {
        "orchestra/testbench": "^5.3",
        "phpunit/phpunit": "^8.4 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Laragle\\": "src/Laragle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laragle\\Core\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laragle\\Auth\\AuthServiceProvider",
                "Laragle\\Auth\\Passwords\\PasswordResetServiceProvider"
            ]
        }
    }
}
